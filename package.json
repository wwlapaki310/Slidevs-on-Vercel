{
  "name": "my-slidev-presentations",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "slidev --open SRE-NEXT-2025/slides.md",
    "dev:system": "slidev --open SLIDEV-SYSTEM/slides.md",
    "build": "npm run clean && npm run build:all && npm run build:index",
    "clean": "rm -rf dist",
    "build:all": "npm run build:sre-next && npm run build:slidev-system",
    "build:sre-next": "slidev build SRE-NEXT-2025/slides.md --out dist/sre-next-2025 --base /sre-next-2025/",
    "build:slidev-system": "slidev build SLIDEV-SYSTEM/slides.md --out dist/slidev-system --base /slidev-system/",
    "build:index": "node scripts/build-index.js",
    "preview": "npx serve dist",
    "export": "npm run export:sre-next-pdf && npm run export:system-pdf",
    "export:sre-next-pdf": "slidev export SRE-NEXT-2025/slides.md --output exports/sre-next-2025.pdf",
    "export:system-pdf": "slidev export SLIDEV-SYSTEM/slides.md --output exports/slidev-system.pdf",
    "format": "slidev format SRE-NEXT-2025/slides.md && slidev format SLIDEV-SYSTEM/slides.md"
  },
  "dependencies": {
    "@slidev/cli": "^52.0.0",
    "@slidev/theme-default": "latest",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "playwright-chromium": "^1.45.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
