{
  "name": "my-slidev-presentations",
  "version": "1.1.0",
  "description": "A system for managing multiple Slidev presentations in one repository",
  "type": "module",
  "scripts": {
    "build": "npm run build:slidev-system && npm run build:index",
    "build:slidev-system": "cd slides/slidev-system/src && npm run build",
    "build:index": "node scripts/build-index.js",
    "dev:slidev-system": "cd slides/slidev-system/src && npm run dev",
    "create-slide": "node scripts/create-slide.js",
    "generate-previews": "node scripts/generate-previews.js"
  },
  "keywords": [
    "slidev",
    "presentation",
    "vercel",
    "vue",
    "markdown",
    "slides",
    "pnpm-workspace",
    "multi-presentation"
  ],
  "author": "Open Source Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wwlapaki310/my-slidev-presentations"
  },
  "devDependencies": {
    "playwright-chromium": "^1.40.0"
  }
}
