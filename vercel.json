{
  "installCommand": "npm run install:all",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/slidev-system/:path*", "destination": "/slidev-system/index.html" },
    { "source": "/slidev-system-zh/:path*", "destination": "/slidev-system-zh/index.html" },
    { "source": "/slidev-system-ja/:path*", "destination": "/slidev-system-ja/index.html" }
  ]
}
