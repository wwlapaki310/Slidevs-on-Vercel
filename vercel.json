{
  "installCommand": "npm run install:all",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/slidev-system/(.*)", "destination": "/slidev-system/index.html" },
    { "source": "/slidev-system-ja/(.*)", "destination": "/slidev-system-ja/index.html" },
    { "source": "/slidev-system-zh/(.*)", "destination": "/slidev-system-zh/index.html" }
  ]
}